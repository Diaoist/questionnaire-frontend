.questionnaire-userinfo-avatar {
  margin-top: 40rpx;
  margin-left: 40rpx;
  background: #d8d8d8;
  border: 1px solid #979797;
  width: 99px;
  height: 99px;
  border-radius: 100%;
}

.title {
  width: 100%;
  height: 108rpx;
  padding-bottom: 8rpx;
  margin-top: 32rpx;
  border-top: 1rpx solid #979797;
  border-bottom: 1rpx dashed #979797;
  font-family: PingFangSC-Regular;
  font-size: 36px;
  color: #000;
  letter-spacing: 0;
  text-align: center;
}

.subtitle {
  width: 100%;
  margin-top: 18rpx;
  padding-bottom: 14rpx;
  border-bottom: 1rpx dashed #979797;
  font-family: PingFangSC-Regular;
  font-size: 24px;
  color: #000;
  letter-spacing: 0;
  text-indent: 98rpx;
  text-align: left;
}

.questionnaire-chooselist {
  margin-bottom: 12rpx;
}

.choose {
  background: #fff;
  border-radius: 8px;
  width: 156px;
  height: 52px;
}
