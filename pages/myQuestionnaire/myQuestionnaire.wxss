/* myQuestionnaire.wxss */

.user-info-avatar {
  margin-left: 40rpx;
  margin-top: 40rpx;
  background: #d8d8d8;
  border: 1px solid #979797;
  width: 99px;
  height: 99px;
  border-radius: 100%;
}

.questionnaire-infolist {
  margin-top: 32rpx;
  width: 100%;
  border-top: 0.5px solid #000;
}

.questionnaire-infolist-overview {
  width: 100%;
  height: 254rpx;
  border-bottom: 0.5px solid #000;
}

.questionnaire-infolist-id {
  float: left;
  margin-top: 28rpx;
  margin-bottom: 70rpx;
  width: 314rpx;
  height: 156rpx;
}

.questionnaire-infolist-id-title {
  width: 314rpx;
  height: 100rpx;
  font-family: PingFangSC-Regular;
  font-size: 36px;
  color: #000;
  letter-spacing: 0;
  text-align: right;
}

.questionnaire-infolist-id-hint {
  width: 314rpx;
  font-family: PingFangSC-Regular;
  font-size: 20px;
  color: #000;
  letter-spacing: 0;
  text-align: right;
}

.questionnaire-infolist-data {
  margin-top: 32rpx;
  margin-bottom: 52rpx;
  float: right;
  width: 180px;
  height: 83px;
}

.statisticsData {
  font-family: PingFangSC-Regular;
  font-size: 20px;
  color: #000;
  letter-spacing: 0;
  text-align: left;
}
